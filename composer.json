{
    "name": "andrewandante/silverstripe-pdf-parser",
    "type": "silverstripe-vendormodule",
    "description": "Adds PDFParser package to extract contents of PDF files for search etc.",
    "require": {
        "php": "^8.3",
        "silverstripe/textextraction": "^5",
        "smalot/pdfparser": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Andrew Paxley",
            "email": "andrew.paxley@silverstripe.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "AndrewAndante\\SilverStripePDFParser\\": "src",
            "AndrewAndante\\SilverStripePDFParser\\Extractor\\Tests\\": "tests"
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "silverstripe/vendor-plugin": true
        }
    }
}
